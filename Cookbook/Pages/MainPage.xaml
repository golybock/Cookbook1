<Page x:Class="Cookbook.Pages.MainPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:Cookbook.Pages"
      xmlns:mv="clr-namespace:Cookbook.Views.Recipe"
      xmlns:ui="http://schemas.modernwpf.com/2019"
      mc:Ignorable="d"
      Title="Рецепты"
      Loaded="MainPage_OnLoaded">
    
    <mv:RecipesListView x:Name="RecipesListView"
                        DeleteClicked="RecipesListView_OnDeleteClicked"
                        EditClicked="RecipesListView_OnEditClicked"
                        OpenClicked="RecipesListView_OnOpenClicked"
                        LikeClicked="RecipesListView_OnLikeClicked"/>
    
</Page>
